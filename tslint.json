{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": {
            "options": [120]
        },
        "indent": [true, "spaces", 2],
        "useTabs": true,
        "semicolon": [true, "always"],
        "no-console": [false],
        "quotemark": [true, "single"]
    },
    "rulesDirectory": [
        "./src"
    ],
    "severity": "warning"
}